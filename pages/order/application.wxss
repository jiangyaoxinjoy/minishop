.app .header_card{
  background: #fff;
}
.app{
  margin-bottom: 56px;
}
.content{
  margin-top: 10px;
}
.footer-fixed {
  position: fixed;
  display: flex;
  bottom: 0px;
  left: 0px;
  right: 0px;
  height: 46px;
  z-index: 18;
  box-shadow: 1px 5px 15px rgba(50, 50, 50, 0.3);
  background: #fff;
}
.order-bt {
  width: 50%;
  background-color: #ee8246;
  color: #fff;
  text-align: center;
  line-height: 46px;
}
.app .repeal_btn{
  color: #ee8246;
  background-color: #fff;
  border-color: #ee8246;
}
.number .van-field__body{
  display: inline-block
}
.number .van-cell__value{
  text-align: left;
}
.number{
  position: relative;
}
.step,.van-stepper{
  position: absolute;
  right: 20px;
  top: 2px;
}
.app .van-cell{
  padding: 8px 20px;
}

.title{
  font-weight: bold;
  font-size: 1.1em;
  position: relative;
}
.title::before{
  content: '';
  display: block;
  position: absolute;
  left: -10px;
  top: 15%;
  height: 70%;
  width: 3px;
  background-color: #ee8246;
  border-radius: 50px;
}
.t_d{
  margin-left: 10px;
  color:#666;
}
.text_explain{
  display: block;
  border: 1px solid #ccc;
  border-radius: 3px;
  margin-left: 0px;
  padding: 10px;
}
.msg{
  color:#666;
  /* font-size: 1em; */
}
.type{
  margin-bottom: 10px;
}
.wrap{
  margin-bottom: 10px;
  padding: 8px 0;
  background: #fff;
}
.show_cell .van-cell{
  padding: 2px 20px;
}
